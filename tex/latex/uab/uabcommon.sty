\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{uabcommon}[2014/03/03 UABCommon Package v0.01]
% Requires packages common to document classes.

% Base package.
\RequirePackage{uabbase}

% Itemization packages.
\RequirePackage{enumitem}
% Some basic lists for enumitem
\newlist{alphalist}{enumerate}{1}
\setlist[alphalist]{label=(\alph*),resume}
\newlist{romanlist}{enumerate}{1}
\setlist[romanlist]{label=(\roman*),resume}
\newlist{numberlist}{enumerate}{1}
\setlist[numberlist]{label=(\arabic*),resume}
\newlist{dotlist}{enumerate}{1}
\setlist[dotlist]{label=\textbullet,resume}
